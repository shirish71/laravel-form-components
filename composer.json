{
    "name": "shirish71/laravel-form-components",
    "description": "form components for tailwind",
    "keywords": [
        "shirish71",
        "laravel-form-components"
    ],
    "homepage": "https://github.com/shirish71/laravel-form-components",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Shirish Maharjan",
            "email": "shirishmaharjan71@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "livewire/livewire": "^2.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Shirish71\\LaravelFormComponents\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shirish71\\LaravelFormComponents\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shirish71\\LaravelFormComponents\\LaravelFormComponentsServiceProvider"
            ]
        }
    }
}
